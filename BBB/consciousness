#!/bin/bash

# initialize error logs
mkdir -p /tmp/errorlogs

# start actuators
initiate

# wake up
nohup /home/atecce/MarsOASIS/BBB/focus > /tmp/errorlogs/startup 2>&1 &

# breathe
nohup /home/atecce/MarsOASIS/BBB/breathe > /tmp/errorlogs/startup 2>&1 &

# keep it clean
nohup /home/atecce/MarsOASIS/BBB/clean > /tmp/errorlogs/startup 2>&1 &
